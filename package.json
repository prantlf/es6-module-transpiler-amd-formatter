{
  "name": "es6-module-transpiler-amd-formatter",
  "version": "0.3.0",
  "description": "ES6 Module Transpiler Extension to Output AMD define() Format.",
  "author": "Caridy Patino <caridy@gmail.com>",
  "homepage": "https://github.com/caridy/es6-module-transpiler-amd-formatter",
  "keywords": [
    "es6",
    "module",
    "transpiler",
    "amd",
    "module-formatter"
  ],
  "bugs": "https://github.com/caridy/es6-module-transpiler-amd-formatter/issues",
  "main": "index.js",
  "files": [
    "lib/",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/caridy/es6-module-transpiler-amd-formatter.git"
  },
  "license": "MIT",
  "dependencies": {
    "recast": "~0.20.3"
  },
  "devDependencies": {
    "requirejs": "^2.3.6",
    "es6-module-transpiler": "^0.10.0",
    "chai": "~4.2.0",
    "mocha": "~8.1.3",
    "nyc": "^15.1.0"
  },
  "scripts": {
    "pretest": "nyc ./node_modules/.bin/compile-modules convert -f ../../../../index.js test/**/*.js -o ./build",
    "test": "mocha"
  },
  "nyc": {
    "checkCoverage": true,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "branches": 60,
    "lines": 80,
    "functions": 70,
    "statements": 80
  }
}
